<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" parallel="tests" thread-count="2">
    <test name="Login Tests">
        <classes>
            <class name="org.expero.tests.LoginTest"/>
        </classes>
    </test>

    <test name="Dashboard Tests">
        <classes>
            <class name="org.expero.tests.DashboardTest"/>
        </classes>
    </test>

    <test name="Page Not Found Tests">
        <packages>
            <package name="org.expero.tests.PageNotFoundTest"/>
        </packages>
    </test>
</suite>